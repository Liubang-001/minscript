# Test for loop with list
my_list = [1, 2, 3, 4, 5]
print("Testing list iteration:")
for x in my_list:
    print(x)

# Test for loop with tuple
my_tuple = (10, 20, 30)
print("Testing tuple iteration:")
for y in my_tuple:
    print(y)

# Test for loop with dict
my_dict = {"a": 1, "b": 2, "c": 3}
print("Testing dict iteration:")
for key in my_dict:
    print(key)

# Test for loop with string
my_string = "hello"
print("Testing string iteration:")
for char in my_string:
    print(char)


